<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog
        id="confirmManualClosingDialog"
        type="Message"
        title="{i18n>ManualClose.title}">
        <content>
            <Text text="{i18n>ManualClose.text}"/>
        </content>
        <beginButton>
            <Button text="{i18n>ManualClose.btn.conf}" press=".onConfermaChiusuraManuale"/>
        </beginButton>
        <endButton>
            <Button text="{i18n>ManualClose.btn.canc}" press=".onCloseDialog"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>